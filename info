public class Apessoa
{

	private String nome;
	private int cpf;

	public Empresario(String nome, int cpf)
	{
		this.nome = nome;
		this.cpf = cpf;
	}
	//Getters e Setters
	public String getNome()
	{
		return nome;
	}
	public void setNome(String nome)
	{
		this.nome = nome;
	}
	public long getCpf()
	{
		return cpf;
	}
	public void setCpf(int cpf)
	{
		this.cnpj = cpf;
	}

}
